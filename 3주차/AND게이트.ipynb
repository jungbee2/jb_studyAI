      "(0,0)= 0\n",
      "(1,0)= 0\n",
      "(0,1)= 0\n",
      "(1,1)= 1\n"
     ]
    }
   ],
   "source": [
    "print('(0,0)=',AND(0,0))\n",
    "print('(1,0)=',AND(1,0))\n",
    "print('(0,1)=',AND(0,1))\n",
    "print('(1,1)=',AND(1,1))"
